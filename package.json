{
  "name": "dot-electron",
  "version": "1.0.0",
  "main": "dist-electron/main.js",
  "description": "Really simple Electron + Vue + Vite.",
  "author": "xiaohai <1372195290@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
